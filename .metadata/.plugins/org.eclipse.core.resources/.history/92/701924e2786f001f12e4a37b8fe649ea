package operadores_relacionais;

public class Operadores_Relacionais {

	public static void main(String[] args) {
		//atribuindo valores
		int x = 10;
		int k = 20;
		//teste lógicos
		System.out.printf("x é igual a 10? %b\n", x == 10);
		System.out.printf("x é diferente de 10? %b\n", x != 10);
		System.out.printf("x é maior que 10? %b\n", x > 10);
		System.out.printf("x é maior ou igual a 10? %b\n", x >= 10);
		System.out.printf("x é menor que 10? %b\n", x < 10);
		System.out.printf("x é menor ou igual 10? %b\n", x <= 10);
	}

}
